{
  "name": "wuf-quick-start",
  "version": "2.2.2",
  "author": "Darren Luvaas",
  "description": "A Quick Start application for use with WUF.",
  "config": {
      "ext_server": "js,ts,html,json"
  },
  "scripts": {
    "ng": "ng",
    "bootstrap": "yarn install",
    "build": "yarn build:prod",
    "build:angular": "ng build --prod",
    "build:copyFiles": "copyfiles -u 1 ./server/data/** ./dist",
    "build:docker": "docker build -t wasvic/${npm_package_name} .",
    "build:prod": "run-s build:angular build:server",
    "build:reset": "rm -rf node_modules && yarn bootstrap && yarn build",
    "build:server": "tsc --project server && yarn build:copyFiles",
    "start": "yarn start:bff",
    "start:bff": "run-s build:server start:dev",
    "start:no-bff": "yarn run:angular",
    "start:dev": "run-p run:angular run:server:dev",
    "start:prod": "run-s build:prod run:server:prod",
    "start:docker": "yarn docker:start",
    "run:angular": "ng serve",
    "run:server:dev": "nodemon --watch server --ext ts --ignore *.spec.ts --exec ts-node --project server/tsconfig.json server/www",
    "run:server:prod": "node dist/www.js",
    "docker:start": "run-s build:docker docker:run",
    "docker:push": "docker tag wasvic/${npm_package_name} wasvic/${npm_package_name}:${npm_package_version} && docker push wasvic/${npm_package_name}:${npm_package_version}",
    "docker:run": "docker run --read-only -v ${PWD}/configuration:/home/node/configuration --rm -p 3000:3000 wasvic/${npm_package_name}",
    "docker:cli": "docker run --read-only -v ${PWD}/configuration:/home/node/configuration -ti wasvic/${npm_package_name} sh",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "extract-i18n": "ng xi18n --output-path src/locale && xliffmerge --profile xliffmerge.json"
  },
  "private": false,
  "dependencies": {
    "@angular/animations": "~8.2.4",
    "@angular/cdk": "~8.2.3",
    "@angular/common": "~8.2.4",
    "@angular/compiler": "~8.2.4",
    "@angular/core": "~8.2.4",
    "@angular/forms": "~8.2.4",
    "@angular/material": "~8.2.3",
    "@angular/platform-browser": "~8.2.4",
    "@angular/platform-browser-dynamic": "~8.2.4",
    "@angular/router": "~8.2.4",
    "@anviltech/wuf-ang-configuration": "^2.0.1",
    "@anviltech/wuf-ang-drawer": "^2.0.1",
    "@anviltech/wuf-ang-layout": "^2.0.1",
    "@anviltech/wuf-ang-navigation": "^2.0.1",
    "@anviltech/wuf-ang-utils": "^2.0.1",
    "@anviltech/wuf-web-assets": "^2.0.1",
    "@anviltech/wuf-web-code-sample": "^2.0.1",
    "@anviltech/wuf-web-message": "^2.0.1",
    "@ngx-translate/core": "^11.0.1",
    "@ngx-translate/http-loader": "^4.0.0",
    "axios": "^0.18.1",
    "body-parser": "^1.18.3",
    "bunyan": "^1.8.12",
    "bunyan-debug-stream": "^2.0.0",
    "classlist.js": "^1.1.20150312",
    "cookie-parser": "^1.4.4",
    "core-js": "^2.5.4",
    "debug": "^4.1.1",
    "express": "^4.16.4",
    "hammerjs": "^2.0.8",
    "intl": "^1.2.5",
    "material-components-web": "~4.0.0",
    "material-design-icons": "3.0.1",
    "pug": "^2.0.3",
    "require-all": "^3.0.0",
    "rxjs": "~6.4.0",
    "zone.js": "~0.9.1"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.803.23",
    "@angular/cli": "~8.3.2",
    "@angular/compiler-cli": "~8.2.4",
    "@angular/language-service": "~8.2.4",
    "@types/express": "^4.16.1",
    "@types/jasmine": "~2.8.8",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "^12.12.11",
    "@types/request": "^2.48.1",
    "codelyzer": "~4.5.0",
    "copyfiles": "^2.1.0",
    "jasmine-core": "~3.4.0",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~4.1.0",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "~2.0.1",
    "karma-jasmine": "~3.1.0",
    "karma-jasmine-html-reporter": "~1.5.1",
    "karma-mocha-reporter": "~2.2.5",
    "ngx-i18nsupport": "^0.8.6",
    "nodemon": "^1.18.10",
    "npm-run-all": "^4.1.5",
    "protractor": "~5.4.2",
    "ts-node": "^8.0.2",
    "tslint": "~5.15.0",
    "typescript": "~3.5.3"
  }
}
